{
  "name": "electron-gql",
  "version": "1.0.0",
  "main": "./index.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "start": "npm run renderer:prod && npm run main:prod",
    "start:dev": "npm-run-all --parallel renderer:dev main:dev",
    "renderer:clear": "rm -rf ./renderer/dist",
    "renderer:dev": "npm run renderer:clear && webpack --mode development --watch --config ./renderer/webpack.config.js",
    "renderer:prod": "npm run renderer:clear && webpack --mode production --config ./renderer/webpack.config.js",
    "main:dev": "NODE_ENV=development electron .",
    "main:prod": "NODE_ENV=production electron ."
  },
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@nerjs/eslint-config": "^1.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "debounce": "^1.2.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^4.0.0",
    "electron": "^10.1.3",
    "fs-extra": "^9.0.1",
    "lodash.get": "^4.4.2",
    "merge": "^1.2.1",
    "nlogs": "^1.1.3",
    "nodemon": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "qs": "^6.9.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-icons-kit": "^1.3.1",
    "react-router-dom": "^5.2.0",
    "react-use": "^15.3.4",
    "slugify": "^1.4.5",
    "styled-components": "^5.2.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.1.0"
  }
}
